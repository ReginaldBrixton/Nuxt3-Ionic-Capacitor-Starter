{
  "name": "nuxt-starter",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.1",
  "license": "MIT",
  "author": {
    "name": "John Doe",
    "email": "johndoe@example.com",
    "url": "https://example.com"
  },
  "engines": {
    "node": ">=18"
  },
  "build": {
    "appId": "{{ appId }}",
    "directories": {
      "buildResources": "static",
      "output": "build"
    }
  },
  "main": ".output/electron/background.js",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev -o",
    "dev:tunnel": "nuxt dev --tunnel",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "format:prettier:check": "prettier --check .",
    "clean": "rm -rf .nuxt .output dist node_modules package-lock.json yarn.lock && pnpm i",
    "ionic:sync": "pnpx cap sync",
    "ionic:run:ios": "npx cap sync && npx cap run ios",
    "ionic:run:android": "npx cap sync && npx cap run android",
    "ionic:open:ios": "npx cap open ios",
    "ionic:open:android": "npx cap open android",
    "ionic:build:android": "ionic capacitor build android",
    "ionic:sync:android": "ionic capacitor sync",
    
    "postinstall": "nuxt prepare && electron-builder install-app-deps",
    "transpile-electron": "tsc -p ./electron --outDir .output/electron",
    "build:electron": "pnpm build && pnpm transpile-electron && node build.js",
    "dev:electron": "NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess 'electron ./.output/electron/background.js'\"",
    "dev2": "cross-env NODE_ENV=DEV electron .",
    "build2": "electron-builder",
    "run.electron": "electron ./.output/electron/background.js",
    "dev:electron:win": "set NODE_ENV=development& concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\"",
    "dev:electron:linux": "set NODE_ENV=development& concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\""
  },
  "dependencies": {
    "@capacitor/android": "^5.7.5",
    "@capacitor/app": "^5.0.7",
    "@capacitor/core": "^5.7.5",
    "@capacitor/haptics": "^5.0.7",
    "@capacitor/ios": "^5.7.5",
    "@capacitor/keyboard": "^5.0.8",
    "@capacitor/status-bar": "^5.0.7",
    "@google/generative-ai": "0.10.0",
    "@ionic/vue": "^8.2.0",
    "@ionic/vue-router": "^8.2.0",
    "@pinia/nuxt": "^0.4.7",
    "@tailwindcss/typography": "^0.5.13",
    "cap": "^0.2.1",
    "cordova-plugin-ionic": "5.5.3",
    "electron-log": "^5.1.5",
    "electron-updater": "^6.1.8",
    "express": "^4.19.2",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.0",
    "@capacitor/cli": "^6.0.0",
    "@nuxt/image": "^1.7.0",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/ionic": "^0.14.1",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "0.59.0-beta.1",
    "@unocss/eslint-plugin": "^0.58.9",
    "@unocss/nuxt": "0.59.0-beta.1",
    "@vueuse/nuxt": "10.9.0",
    "concurrently": "^8.2.2",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.3",
    "electron": "^23.3.13",
    "electron-builder": "^23.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^7.16.0",
    "nuxt": "^3.11.2",
    "nuxt-electron": "^0.7.0",
    "nuxt-icon": "0.6.10",
    "pinia": "^2.1.7",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "wait-on": "^7.0.1"
  }
}
